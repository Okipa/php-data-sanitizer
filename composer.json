{
    "name": "okipa/php-data-sanitizer",
    "description": "Data sanitizer to auto-cast entries, convert empty strings to null, etc.",
    "keywords": [
        "okipa",
        "package",
        "php",
        "data",
        "clean",
        "sanitize",
        "sanitizer",
        "laravel",
        "codeigniter",
        "fuelphp"
    ],
    "homepage": "https://github.com/Okipa/php-data-sanitizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Lorent",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Daniel Lucas",
            "email": "daniel.chris.lucas@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "phpunit/phpunit": "^8.0",
        "phpmd/phpmd": "^2.0",
        "squizlabs/php_codesniffer": "^3.3",
        "nunomaduro/larastan": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\DataSanitizer\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd src text phpmd.xml",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\DataSanitizer\\Laravel\\DataSanitizerServiceProvider"
            ],
            "aliases": {
                "DataSanitizer": "Okipa\\DataSanitizer\\Laravel\\Facades\\DataSanitizer"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
